{
    "compilerOptions": {
      "target": "es2017",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["dom", "esnext"],
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": [
      "src/**/*",
      "node_modules/@sveltejs/kit/types.d.ts",
      "node_modules/svelte/types/runtime/*",
      "node_modules/svelte/*"
    ]
  }
  